<script>
    //*** chartbeat implementation

    var _sf_async_config={};
    /** CONFIGURATION START **/
    _sf_async_config.uid = 45577;
    _sf_async_config.domain = 'sport1.fm';
    _sf_async_config.useCanonical = true;
    _sf_async_config.sections = 'sport1.fm';
    _sf_async_config.authors = 'sport1.fm';
    /** CONFIGURATION END **/
    (function(){
      function loadChartbeat() {
        window._sf_endpt=(new Date()).getTime();
        var e = document.createElement('script');
        e.setAttribute('language', 'javascript');
        e.setAttribute('type', 'text/javascript');
        e.setAttribute('src', '//static.chartbeat.com/js/chartbeat.js');
        document.body.appendChild(e);
      }
      var oldonload = window.onload;
      window.onload = (typeof window.onload != 'function') ?
         loadChartbeat : function() { oldonload(); loadChartbeat(); };
    })();
</script>

<!-- BSN fm Pixel -->
<script type="text/javascript">
if (/http:\/\/.*?sport1.fm\/*$/.test(location.href)) {
	var Zeitraumstart = "19.04.2014";
	var Zeitraumende = "10.05.2014";

	var jetzt = new Date();
	var Jahr = jetzt.getFullYear();
	var Monat = jetzt.getMonth()+1;
	if (Monat <= 9) { var Monat = "0"+Monat; }
	var Tag = jetzt.getDate();
	if (Tag <= 9) { var Tag = "0"+Tag; }
		
	var showBSN = false;
		
	var Datecheck = ""+Tag+"."+Monat+"."+Jahr+"";
	var ConDatecheck = Date.parse(Datecheck);
		
	// Prüfung ob BSN fm Pixel aktiv ist
	if (Datecheck >= Zeitraumstart) { showBSN = true; }
	if (Datecheck <= Zeitraumende) { showBSN = true; }
		
	// Ausgabe BSN fm Pixel
	if (showBSN == true) {
	document.write('<div style="position: absolute; width: 1px; height: 1px;">');
	document.write('<img src="http://slogantrend.de/Standard/45Tn33/ever/kampagnen_SPO0114_2.php?vermarkter=Sport1&seite=Sport1FM&platzierung=Sport1&werbe=Misch&studie=0114_SP1&kunde=Sport1" style="border:0px #FFFFFF none;" name="myiframe" scrolling="no" frameborder="0" marginheight="0px" marginwidth="0px" height="1" width="1"></img>');
	document.write('</div>');
	}
}
</script>
<!-- // BSN fm Pixel -->
</body>
</html>

